<h1>02_keyArr</h1>
<script>
var arr1 = {'red':100, 'blue':'파랑',100:[11,22,33],'red':'빨강',300:'파랑'}

//보기
document.write(arr1+"<br>")
console.log(arr1)
document.write(arr1['red']+"<br>")
document.write(arr1.red+"<br>")
document.write(arr1[0]+"<br>")

//추가
arr1['yellow'] = '노랑'
arr1.pink = '핑크'

//삭제
delete arr1['100']
delete arr1.blue

console.log(arr1)
document.write("---------------------------<br>")
//for
for(var kk in arr1){
	
	document.write(kk+"<br>")
}



</script>
</body>
</html>