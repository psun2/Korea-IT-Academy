package jdbc_p;

import java.util.ArrayList;

public class ExamMain {

	public static void main(String[] args) {

		ArrayList<ExamDTO> arr = new ArrayList<ExamDTO>();
		arr.add(new ExamDTO(50, "중간", "aaa", "2019-05-08", 62,64,68));
		arr.add(new ExamDTO(51, "기말", "ccc", "2019-05-08", 52,54,58));
		arr.add(new ExamDTO(52, "소말", "ddd", "2019-05-08", 92,94,98));
		arr.add(new ExamDTO(53, "중간", "eee", "2019-05-08", 72,74,78));
		new ExamDAO().insert_1(arr);
		
		
		for (ExamDTO dto: new ExamDAO().list()) {
			System.out.println(dto);
		}
		
		System.out.println("--------------------");
		

	}

}
