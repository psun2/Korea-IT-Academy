<script>
document.write('<h2>1. 산술연산자</h2>')
var a = 15, b = 7
document.write(a+b+"<br>")
document.write(a-b+"<br>")
document.write(a*b+"<br>")
document.write(a/b+"<br>")
document.write(a%b+"<br>")
a = '정우성'
b = '현빈'
document.write(a+b+"<br>")
document.write(a-b+"<br>")
document.write(a*b+"<br>")
document.write(a/b+"<br>")
document.write(a%b+"<br>")
document.write('<h2>2. 증감연산자</h2>')
a = 5
document.write(a +"<br>")
document.write(a++ +"<br>")
document.write(++a +"<br>")
document.write(a +"<br>")
document.write(a-- +"<br>")
document.write(--a +"<br>")
document.write(a +"<br>")
//document.write(a** +"<br>")
//document.write(**a +"<br>")

</script>
