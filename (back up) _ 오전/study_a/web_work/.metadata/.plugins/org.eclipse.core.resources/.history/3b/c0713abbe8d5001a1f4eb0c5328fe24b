//var ii = confirm("저장하시겠습니까?")
//document.write("ii:"+ii+"<br>")

var a = "2+3*4"
document.write("a:"+a+"<br>")
document.write("a:"+eval(a)+"<br>")
document.write(eval("fn_1(15,9)")+"<br>")


var b = "1234"
document.write(parseInt(b)+100+"<br>")
document.write(parseInt("123.456")+100+"<br>")
document.write(parseFloat("123.456")+100+"<br>")

document.write(isNaN("asdfasf")+"<br>")
document.write(isNaN(1234)+"<br>")
document.write(isNaN("4567")+"<br>")
document.write(isNaN(true)+"<br>")
document.write(isNaN(false)+"<br>")
document.write(isNaN("1234asdf")+"<br>")
document.write(isNaN("100+200")+"<br>")

function aaa(){
	document.write("3초 뒤에 출력할껄?");
}

aaa();


